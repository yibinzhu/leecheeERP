<view class="weui-cells weui-cells_after-title">
<view class="weui-cell" wx:for="{{adminData}}" wx:for-item="item" wx:key='id' wx:for-index="idx">
<view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
    <image src="{{item.avatarUrl?item.avatarUrl:'../../../images/pic_160.png'}}" style="width: 50px; height: 50px; display: block"/>
    <!-- <view class="weui-badge" style="position: absolute;top: -.4em;right: -.4em;">8</view> -->
</view>
<view class="weui-cell__bd">
    <view>{{item.nickName}}</view>
    <view style="font-size: 13px;color: #888888;">{{item.gender==1?"男":"女"}}</view>
</view>
</view>
</view>