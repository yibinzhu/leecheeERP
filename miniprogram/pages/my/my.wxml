        <view class="weui-cells weui-cells_after-title" style='margin-top:5px'>
            <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd"><view class="weui-label">手机号码</view></view>
                <view class="weui-cell__bd"><input class="weui-input" placeholder="请输入手机号码" type='number' value='{{phoneNum}}' bindblur='savePhoneNum'/></view>
                <view class='weui-cell_ft'><view style="border:1px solid #000000;border-radius: 5px;width:45px;height:25px;text-align:center" data-flag="0" bindtap='alertSave'><text style='font-size:12px;'>保存</text></view></view>
            </view>
            <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd"><view class="weui-label">配送地址</view></view>
                <view class="weui-cell__bd"><input class="weui-input" placeholder="请输入配送地址" value='{{deliverAddress}}' bindblur='saveDeliverAddress'/></view>
                <!-- <view class='weui-cell_ft'><button type='default' plain="true" size='mini'>保存</button></view> -->
                 <view class='weui-cell_ft'><view style="border:1px solid #000000;border-radius: 5px;width:45px;height:25px;text-align:center" data-flag="1" bindtap='alertSave'><text style='font-size:12px;'>保存</text></view></view>
            </view>

            <view class='weui-cell'>
                 <view class='weui-well_hd'><view class="weui-label">客服电话</view></view>
                 <view class='weui-cell__bd'>01058478965</view>
            </view>
       
        </view>

<button class="weui-btn" type="primary" bindtap="login">登陆</button>